generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
  relationMode = "prisma"
}

model Maintenance {
  id String @id @default(cuid())
  name String 
  merk String
  type String
  serialnum String
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}

model Calibration {
  id String @id @default(cuid())
  name String 
  merk String
  type String
  serialnum String
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt

}

model Products {
  id String @id @default(cuid())
  name String
  category String
  compatibility String
  price String
  stock String
  description String
  link String
}